# Default values for PHP, Magento, and OpenSearch versions
# See: https://experienceleague.adobe.com/en/docs/commerce-operations/installation-guide/system-requirements
PHP_VERSION=8.2
MAGENTO_VERSION=2.4.7
OPENSEARCH_VERSION=2.12.0
MYSQL_VERSION=8.0
RABBITMQ_VERSION=3.9
RECTOR_VERSION=0.17.13

# Path to the Magento module
MODULE_PATH=./src/Vendor/Module

# MySQL configuration
MYSQL_USER=magento
MYSQL_PASSWORD=magento
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=magento

# Magento Access Keys
MAGENTO_PUBLIC_KEY=<secret>
MAGENTO_PRIVATE_KEY=<secret>